{
  "name": "personal-dictionary",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-10k": "node scripts/generate-10k-words.js",
    "test": "vitest --reporter=verbose --environment=jsdom --globals",
    "test:ui": "vitest --ui --environment=jsdom --globals",
    "test:coverage": "vitest --coverage --environment=jsdom --globals",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "random-words": "^2.0.1",
    "vue": "^3.5.18",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^15.2.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^2.1.5",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  }
}
